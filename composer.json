{
    "name": "infast-team/szymone",
    "type": "project",
    "description": "Team menagment system.",
    "keywords": ["infast", "team"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.1",
        "doctrine/dbal": "*",
        "fideloper/proxy": "*",
        "fruitcake/laravel-cors": "*",
        "guzzlehttp/guzzle": "*",
        "laravel/framework": "*",
        "laravel/tinker": "*",
        "laravel/ui": "*",
      "ext-pdo": "*",
      "ext-mongodb": "*"
    },
    "require-dev": {
        "facade/ignition": "*",
        "fakerphp/faker": "*",
        "ibex/crud-generator": "*",
        "laravel/sail": "*",
        "mockery/mockery": "*",
        "nunomaduro/collision": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
